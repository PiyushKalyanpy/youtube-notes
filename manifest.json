{
    "name" : "Youtube Notes",
    "version" : "0.1.0",
    "description" : "Youtube Notes",
    "permission" : ["tabs", "storages"],
    "host_permission" : ["https://*.youtube.com/*"],
    "background" : {
        "service_worker" : "background.js"
    },
    "content_scripts" : {
        "matches" : ["https://*.youtube.com/*"],
        "js" : ["contentScript.js"]
    },
    "web_accessible_resources" : {
        "resources" : [
            "assets/bookmark.png",
            "assets/delete.png",
            "assets/play.png",
            "assets/pause.png",
            "assets/stop.png",
            "assets/save.png"
        ],
        "matches" : ["https://*.youtube.com/*"]
    },
    "manifest_version" : 3,
    "action" : {
        "default_icon" : {
            "16" : "assets/icon16.png",
            "48" : "assets/icon48.png",
            "128" : "assets/icon128.png"
        },
        "default_title" : "Youtube Notes",
        "default_popup" : "popup.html"
    }
}